## Set Clang as default compiler set
TOOLCHAIN=gnu

## Enable LTO both for kernel and applications
LTOENABLE=1
KERNEL_LTOENABLE=0

## Enable shared libraries (default) but disable static ones
BUILD_LIBSHARED=1
BUILD_LIBSTATIC=1

setup_sccache
setup_gcc --install
#setup_llvm
build microsoft/wsl-kernel_5.15

cp -fv ${BIN_PATH}/boot/vmlinuz* ${DATA_PATH}/${PRJ_NAME}/${PLATFORM_NAME}