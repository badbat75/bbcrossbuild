# wireless-regdb-2019-03-01
PKG_URL="https://git.kernel.org/pub/scm/linux/kernel/git/sforshee/wireless-regdb.git/snapshot/wireless-regdb-master-2019-03-01.tar.gz"
BUILD_PROCESS=simplemake
INST_CMD=install
PKG_DEPS=
PKG_PREBUILD="cp ${DATA_PATH}/pki/${PKI_KEY_NAME}.x509.pem . &&
openssl rsa -in ${PKI_KEY_NAME}.x509.pem -out ${PKI_KEY_NAME}.key.pub.pem"
PKG_MAKEVARS="PREFIX=\${BUILD_PREFIX} CRDA_PATH=/lib/crda CRDA_KEY_PATH=/lib/crda/pubkeys FIRMWARE_PATH=/lib/crda MANDIR=\${BUILD_PREFIX}/share/man REGDB_PRIVKEY=${DATA_PATH}/pki/${PKI_KEY_NAME}.x509.pem REGDB_AUTHOR=${PKI_KEY_NAME}"
