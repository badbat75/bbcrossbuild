# alsa-lib_1.2.6.1
GIT_URL="https://github.com/alsa-project/alsa-lib"
GIT_COMMIT="tags/v1.2.6.1"
BUILD_PROCESS=configmake
AUTOCONF=1
PKG_DEPS="moode/alsa-ucmconf_1.2.6.1"
CONF_FLAGS=""
INST_CMD=install-strip
[ "${LTOENABLE:-0}" != "0" ] && PKG_OVERRIDELTO=fat || true
PKG_PREBUILD="sed -i 's/ln -s/ln -fs/g' include/Makefile.am"

case ${TOOLCHAIN} in
    gnu) PKG_CFLAGS="-flto-partition=none";;
    llvm) ;;
esac
