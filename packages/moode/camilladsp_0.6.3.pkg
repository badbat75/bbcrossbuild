# camilladsp_0.6.3
PKG_URL="https://github.com/HEnquist/camilladsp/archive/v0.6.3.tar.gz"
BUILD_PROCESS=cargobuild
CONF_FLAGS="--no-default-features --features alsa-backend --features websocket"
#PKG_RUSTFLAGS=
PKG_DEPS="moode/alsa-lib_1.2.6.1"
#CARGO_BIN=/usr/bin
#CARGO_LIB=/usr/lib/${HARCH}
CARGO_BINLIST="camilladsp"
PKG_LDFLAGS=-s
PKG_POSTBUILD="
	mkdir -p ${PKG_PKGPATH}/usr/local/bin
	ln -fs /usr/bin/camilladsp ${PKG_PKGPATH}/usr/local/bin/camilladsp
	"