# allo-piano-firmware-master
PKG_URL="https://github.com/allocom/piano-firmware/archive/master.tar.gz"
BUILD_PROCESS=downloadonly
PKG_PREBUILD="install -d -m0755 ${BIN_PATH}/lib/firmware/allo/2.1 &&
install -d -m0755 ${BIN_PATH}/lib/firmware/allo/2.2 &&
install -m 0644 -D lib/firmware/allo/2.1/* ${BIN_PATH}/lib/firmware/allo/2.1/ &&
install -m 0644 -D lib/firmware/allo/2.2/* ${BIN_PATH}/lib/firmware/allo/2.2/"