# RPi.GPIO
BUILD_PROCESS=pythonpip
PKG_TOOLCHAIN=gnu
[ ${TOOLCHAIN}=llvm ] && PKG_CFLAGS="-Wno-implicit-function-declaration -Wno-deprecated-declarations"

