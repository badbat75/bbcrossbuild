# bs_libxcrypt
PKG_URL="https://github.com/besser82/libxcrypt/archive/v${BS_LIBXCRYPT_VER:-4.4.27}.tar.gz"
BUILD_PROCESS=configmake
PKG_OVERRIDELTO=0
CONF_FLAGS="--disable-failure-tokens --disable-valgrind \
	--enable-hashes=all --enable-obsolete-api=glibc --enable-obsolete-api-enosys=no"
INST_CMD=install-strip

TOOLCHAIN_LD=ld.bfd