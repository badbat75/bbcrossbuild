# e2fsprogs
PKG_VER=1.46.5
PKG_URL="https://downloads.sourceforge.net/project/e2fsprogs/e2fsprogs/v${PKG_VER}/e2fsprogs-${PKG_VER}.tar.gz"
PKG_DEPS="lfs/util-linux"
#PKG_DEPS+=" lfs/systemd"
BUILD_PROCESS=configmake
CONF_FLAGS="--disable-option-checking --with-root-prefix=\"\" \
    --enable-elf-shlibs --disable-libblkid --disable-libuuid --disable-uuidd --disable-fsck"
PKG_TWOSTEPSBUILD=1
INST_CMD="-j1 install install-strip"
