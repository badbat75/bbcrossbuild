# libnghttp2
PKG_VER=1.51.0
PKG_URL="https://github.com/nghttp2/nghttp2/releases/download/v${PKG_VER}/nghttp2-${PKG_VER}.tar.xz"
BUILD_PROCESS=cmakebuild
VAR_INSTALL_LIBDIR=CMAKE_INSTALL_LIBDIR
VAR_INSTALL_INCDIR=CMAKE_INSTALL_INCLUDEDIR
CONF_FLAGS="-DPYTHON_EXECUTABLE:FILEPATH=$(which python3) -DPYTHON_LIBRARY:FILEPATH=${GLOBAL_TOOLCHAIN_PATH}/python-${PYTHON_VER}/lib/libpython3.so"
INST_CMD="install/strip"
