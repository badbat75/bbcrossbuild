# bs_libxcrypt
PKG_SUFFIX=_${LIBXCRYPT_VER}
PKG_URL="https://github.com/besser82/libxcrypt/archive/v${LIBXCRYPT_VER}.tar.gz"
PKG_AUTOCONF=1
BUILD_PROCESS=configmake
PKG_OVERRIDELTO=0
CONF_FLAGS="--disable-werror --disable-failure-tokens --disable-valgrind \
	--enable-hashes=all --enable-obsolete-api=glibc --enable-obsolete-api-enosys=no"
INST_CMD=install-strip
